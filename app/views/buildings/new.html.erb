
<br>
<%=flash[:building_added]%><br>
<%=flash[:building_not_added]%><br>
Create a New Building
<br>
<hr>
<%= form_for(@building) do |f| -%>
        <%= f.label :name %><br>
        <%= f.text_field(:name)%><br>
        <%= f.label :address %><br>
        <%= f.text_field(:address)%><br>
        <%= f.label :city %><br>
        <%= f.text_field(:city)%><br>
        <%= f.label :state %><br>
        <%= f.text_field(:state)%><br>
        <%= f.label :zip_code %><br>
        <%= f.text_field(:zip_code)%><br>
        <%= f.label :description %><br>
        <%= f.text_field(:description)%><br>
        <%= f.label :owner_id %><br>
        <%= f.collection_select :owner_id, Owner.all, :id, :last_name, :include_blank => 'Add Owner Later'%><br>
        <%= f.submit "Create Building" %>
<%end%>